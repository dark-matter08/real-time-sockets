{
    "swagger": "2.0",
    "info": {
      "title": "Real Time Socket",
      "description": "practical business case for CAMSOL interview",
      "version": "1.0"
    },
    "produces": ["application/json"],
   "securityDefinitions": {
     "api_key": {
       "type": "apiKey",
       "name": "api_key",
       "in":"header"
